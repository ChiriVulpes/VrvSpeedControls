{
	"manifest_version": 2,
	"name": "VRV Speed Controls",
	"short_name": "VrvSpeedControls",
	"version": "1.1.0",
	"content_scripts": [{
		"matches": ["https://static.vrv.co/*"],
		"js": ["index.js"],
		"all_frames": true
	}],
	
	"permissions": [
		"storage"	
	],

    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}